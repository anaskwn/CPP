#include <iostream>
using namespace std;
int main() {
    // Внешний цикл для перебора строк от 1 до 9
    for (int i{1}; i < 10; i++) {
        // Внутренний цикл для перебора столбцов от 1 до 9
        for (int k(1); k < 10; k++) {
            // Выводим произведение i и k, форматируем с помощью табуляции
            cout << i * k << "\t";
        }
        // Переход на следующую строку после завершения внутреннего цикла
        cout << endl;
    }

    return 0;
}
